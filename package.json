    "firebase": "^9.22.2",
    "firebase-admin": "^11.9.0",
    "http-status-codes": "^2.2.0",
    "serve-static": "^1.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "babel-jest": "^29.5.0",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
